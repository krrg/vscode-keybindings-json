// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "home",
        "command": "cursorHome",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+a",
        "command": "cursorHome",
        "when": "editorTextFocus"
    },
    {
        "key": "end",
        "command": "cursorEnd",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+e",
        "command": "cursorEnd",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+a",
        "command": "editor.action.selectAll"
    },
    {
        "key": "ctrl+n",
        "command": "explorer.newFile"
    }
]